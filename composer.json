{
  "name": "akens/laravel-helpers",
  "description": "A library of helpers for use with the Laravel framework.",
  "authors": [
    {
      "name": "Geoff Akens",
      "email": "geoff@geoffakens.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.1.*",
    "illuminate/hashing": "5.1.*",
    "doctrine/dbal": "2.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "phpspec/phpspec": "~2.1",
    "mockery/mockery": "~0.9",
    "sami/sami": "~3.2.1"
  },
  "autoload": {
    "psr-0": {
      "Akens\\LaravelHelpers\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
